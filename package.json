  "private": true,
  "description": "little-brother",
  "keywords": [
    "gatsby"
  ],
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "data:bills": "json-server --watch src/data/bills-118.json --port 3000",
